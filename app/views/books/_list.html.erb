  <div class="row mt-3">
    <%= form_with model: book do |f| %>
      <h2>New book</h2>
      <p>Title</p>
      <%= f.text_field :title %>
      <p>Opinion</p>
      <%= f.text_area :body %>
      <div class="mt-2">
      <%= f.submit "Create Book",class: "btn btn-success" %>
      </div>
    <% end %>
  </div>